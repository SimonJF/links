fun go() {
  var ap = new();
  try {
    var s = fork(fun(s) {
      var t = request(ap);
      ignore(receive(s));
      ignore(send(5, t))
    });
    cancel(s);
    var t = accept(ap);
    var (x, _) = receive(t);
    x
  } as (x) in {
    print(intToString(x))
  } otherwise {
    print("error!")
  }
}

go()
